<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FDFDFD">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="307dp"
            android:layout_height="65dp"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/giveaway"
            android:text="@string/title_settings"
            android:textSize="38sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ScrollView
            android:layout_width="409dp"
            android:layout_height="0dp"
            android:paddingLeft="30dp"
            android:paddingTop="30dp"
            android:paddingRight="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="0.0">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="700dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView26"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:text="@string/regresar_menu"
                    android:textSize="22sp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginBottom="50dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868"
                    android:onClick="SiguienteCatorce">

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/hogar"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView23"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/menu_principal"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView4"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/textView24"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:text="@string/sobre_valorandome"
                    android:textSize="22sp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868"
                    android:onClick="SiguienteTrece">

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/ic_baseline_menu_book_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView27"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/b_ayuda"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView7"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868">

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/ic_baseline_menu_book_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView25"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/sobre_nosotros"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView6"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginBottom="50dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868"
                    android:onClick="SiguienteOnce">

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/ic_baseline_menu_book_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView28"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/politicas"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView8"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/textView36"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:text="@string/sobre_ti"
                    android:textSize="22sp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginBottom="20dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868">

                    <ImageView
                        android:id="@+id/imageView9"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/ic_baseline_settings_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView29"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/eliminar"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView9"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:background="@drawable/shape_cinco"
                    android:backgroundTint="#F46868">

                    <ImageView
                        android:id="@+id/imageView10"
                        android:layout_width="74dp"
                        android:layout_height="45dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="1"
                        android:src="@drawable/ic_baseline_settings_24"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.36" />

                    <TextView
                        android:id="@+id/textView30"
                        android:layout_width="217dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:text="@string/log_out"
                        android:textSize="22sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.931"
                        app:layout_constraintStart_toEndOf="@+id/imageView10"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintVertical_bias="0.0" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>